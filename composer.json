{
    "name": "vismutx/klarna-order_management_api-php",
    "description": "Klarna order management api for PHP generated by swagger",
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "",
    "license": "MIT",
    "authors": [
        {
            "name": "Klarna api",
            "homepage": "https://developers.klarna.com/api/#order-management-api"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2||^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.12"
    },
    "autoload": {
        "psr-4": { "Vismutx\\KlarnaOrderManagementApiPhp\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Vismutx\\KlarnaOrderManagementApiPhp\\" : "test/" }
    }
}
